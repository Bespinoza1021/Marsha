#!/usr/bin/env python

import rospy
import roslaunch
import os

from std_srvs.srv import Trigger, TriggerResponse

def reset(req):
    success = True
    try:
        os.system("roslaunch marsha_gazebo spawn_pickup.launch")
    except:
        success = False
    return TriggerResponse(success=success)

def server():
    rospy.init_node('reset_server')
    s = rospy.Service('reset', Trigger, reset)
    rospy.spin()

if __name__ == "__main__":
    server()