<robot name="object1">
  <macro name="box_inertia" params="m x y z">
    <inertia  ixx="${m*(y*y+z*z)/12}" ixy = "0" ixz = "0"
              iyy="${m*(x*x+z*z)/12}" iyz = "0"
              izz="${m*(x*x+z*z)/12}" /> 
  </macro>
  <link name="object1">
    <inertial>
      <origin xyz="0.5 0 0.5" /> 
      <mass value="1.0" />
      <box_inertia m="1.0" x="0.04" y="0.04" z="0.2"/>
    </inertial>
    <visual>
      <origin xyz="0.5 0 0.5"/>
      <geometry>
        <box size="0.04 0.04 0.2" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.5 0 0.5"/>
      <geometry>
        <box size="0.04 0.04 0.2" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.5</mu>
            <mu2>0.5</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>
  <gazebo reference="object1">
    <material>Gazebo/Green</material>
  </gazebo>
</robot>